<script>
  let seconds = 7; // ‚Üê ‡§Ø‡§π‡•Ä ‡§¨‡§¶‡§≤‡§®‡§æ ‡§π‡•à timing ‡§ï‡•á ‡§≤‡§ø‡§è (5, 10, 15 etc.)
  let countdownEl = document.getElementById("countdown");
  let progressBar = document.getElementById("progress-fill");

  let totalTime = seconds;

  let interval = setInterval(() => {
    seconds--;
    countdownEl.textContent = seconds;

    let progress = ((totalTime - seconds) / totalTime) * 100;
    progressBar.style.width = progress + "%";

    if (seconds <= 0) {
      clearInterval(interval);
      window.location.href = "https://naukripulse.blogspot.com/p/result.html";
    }
  }, 1000);
</script><style>
